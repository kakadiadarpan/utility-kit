{
  "name": "utililty-kit",
  "version": "0.1.2",
  "description": "A collection of tiny JavaScript utilities.",
  "repository": "https://github.com/kakadiadarpan/utility-kit.git",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "run-s clean && run-p build:*",
    "build:es": "NODE_ENV=production TARGET=es rollup -c",
    "build:umd": "NODE_ENV=production TARGET=umd rollup -c",
    "precommit": "lint-staged",
    "bundlesize": "bundlesize"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "author": "Darpan Kakadia <kakadiadarpan@gmail.com>",
  "license": "MIT",
  "keywords": [
    "json",
    "object",
    "array",
    "deep",
    "nested",
    "path",
    "kakadiadarpan",
    "javascript",
    "type",
    "collection",
    "function",
    "util",
    "utils",
    "string",
    "empty",
    "map"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "bundlesize": "^0.17.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.1.3",
    "rollup": "^1.7.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-plugin-uglify-es": "^0.0.1"
  },
  "dependencies": {},
  "bundlesize": [
    {
      "path": "./dist/index.js",
      "maxSize": "1.1 kB"
    }
  ]
}
